{
  "name": "@ptm/multiregexp",
  "version": "0.0.1",
  "exports": "./jsr/mod.ts",
  "publish": {
    "include": [
      "LICENSE",
      "README.md",
      "jsr/match.ts",
      "jsr/mod.ts",
      "deno.json"
    ],
    "exclude": ["jsr/test"]
  },
  "exclude": ["src/", "tests/"],
  "test": {
    "include": ["jsr/test/mod.test.ts"]
  },
  "compilerOptions": {
    "noImplicitAny": true
  },
  "lint": {
    "include": ["jsr/"]
  },
  "fmt": {
    "useTabs": true,
    "lineWidth": 80,
    "indentWidth": 4,
    "semiColons": false,
    "singleQuote": true,
    "proseWrap": "preserve",
    "include": ["jsr/"]
  },
  "lock": false,
  "imports": {
    "@std/testing": "jsr:@std/testing@^1.0.15"
  }
}
